{
  "tags": [
    {
      "name": "Evidence",
      "description": "证据"
    }
  ],
  "definitions": {
  },
  "paths": {
    "/capture": {
      "get": {
        "description": "关联组态控件与物模型",
        "parameters": [
          {
            "name": "productid",
            "in": "query",
            "type": "string",
            "default": "JVANH8bf5n",
            "description": "组态控件的产品Id"
          },
          {
            "name": "topoid",
            "in": "query",
            "type": "string",
            "default": "330980",
            "description": "组态控件的句柄"
          },
          {
            "name": "thingid",
            "in": "query",
            "type": "string",
            "default": "temper",
            "description": "物模型的数据标识"
          },
          {
            "name": "subdevid",
            "in": "query",
            "type": "string",
            "default": "JVANH8bf5n",
            "description": "物模型的子产品Id"
          }
        ],
        "responses": {
          "200": {
            "description": "Returns operation status"
          },
          "400": {
            "description": "Bad Request"
          },
          "403": {
            "description": "Forbidden"
          },
          "500": {
            "description": "Server Internal error"
          }
        },
        "summary": "关联组态控件与物模型",
        "tags": [
          "Evidence"
        ]
      }
    },
